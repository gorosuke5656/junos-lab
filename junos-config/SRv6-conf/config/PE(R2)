policy-options {
    policy-statement CE2_v4 {
        term 1 {
            from {
                protocol direct;
                route-filter 10.20.0.0/24 exact;
            }
            then {
                next-hop 2001:db8:0:b0:d01::;
                accept;
            }
        }
    }
}
protocols {
    bgp {
        group SRv6 {
            type internal;
            local-address 2001:db8:12::2;
            family inet {
                unicast;
            }
            export CE2_v4;
            neighbor 2001:db8:12::1 {
                family inet {
                    unicast {
                        extended-nexthop;
                    }
                }
            }
        }
        group to_PE2 {
            type internal;
            local-address 2.2.2.2;
            neighbor 1.1.1.1 {
                family inet {
                    unicast;
